<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<context-param>
		<param-name>globalMail</param-name>
		<param-value>6tharshi@gmail.com</param-value>
	</context-param>
	<servlet>
		<servlet-name>RequestParams.do</servlet-name>
		<servlet-class>foo.RequestParams</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StandardActionsWithObjectTypeParams.do</servlet-name>
		<servlet-class>foo.StandardActionsWithObjectTypeParams</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HobbyPage</servlet-name>
		<servlet-class>foo.HobbyServ</servlet-class>
		<jsp-file>/TestBean2.jsp</jsp-file>
		<init-param>
			<param-name>email</param-name>
			<param-value>test@gmail.com</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>CookieCreator.do</servlet-name>
		<servlet-class>foo.CookieCreator</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>checkcookie.do</servlet-name>
		<servlet-class>foo.CookieReader</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>elexercise.do</servlet-name>
		<servlet-class>foo.ELExercise</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>checkcookie.do</servlet-name>
		<url-pattern>/checkcookie.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CookieCreator.do</servlet-name>
		<url-pattern>/CookieCreator.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HobbyPage</servlet-name>
		<url-pattern>/HobbyPage.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StandardActionsWithObjectTypeParams.do</servlet-name>
		<url-pattern>/StandardActionsWithObjectTypeParam.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RequestParams.do</servlet-name>
		<url-pattern>/RequestParams.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>elexercise.do</servlet-name>
		<url-pattern>/ELExercise.do</url-pattern>
	</servlet-mapping>
</web-app>